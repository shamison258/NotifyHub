# Routes

# HomeController
GET   /                         controllers.HomeController.index
GET   /admin                    controllers.HomeController.admin
GET   /user/:id                 controllers.HomeController.user(id: Long)

# AuthController
GET   /createUser               controllers.AuthController.inputUserInfo
POST  /createUser               controllers.AuthController.insertUser
GET   /deleteUser               controllers.AuthController.deleteUser
GET   /login                    controllers.AuthController.login
GET   /logout                   controllers.AuthController.logout

# Map static resources from the /public folder to the /assets URL path
GET   /assets/*file             controllers.Assets.versioned(path="/public", file: Asset)


